{
  "main": "index.js",
  "scripts": {
    "build-storybook": "build-storybook",
    "clear:cache": "npx react-native start --reset-cache",
    "customize:icon": "iconset create",
    "customize:splashscreen": "npx run expo-splash-screen",
    "customize:makeityours": "trash './ios' './android' 'app.json' && expo eject --npm",
    "new:component": "hygen component new",
    "new:screen": "hygen screen new",
    "new:type": "hygen type new",
    "new:util": "hygen util new",
    "prepush": "npm run test:ci",
    "prepare": "husky install",
    "prestorybook": "rnstl",
    "start": "npx react-native start",
    "start:android": "npx react-native run-android",
    "start:ios": "npx react-native run-ios",
    "start:web": "expo start --web",
    "start:clear:cache": "npx react-native start --reset-cache",
    "start:production": "cross-env NODE_ENV=production npm run start:clear:cache",
    "start:development": "cross-env NODE_ENV=development npm run start:clear:cache",
    "start:local": "cross-env NODE_ENV=local npm run start:clear:cache",
    "start:visual": "run-p start:visual:*",
    "start:visual:react-native": "cross-env RENDER_STORYBOOK=true run-s prestorybook start:clear:cache ",
    "start:visual:storybook": " (adb reverse tcp:7007 tcp:7007 || true) && npm run storybook",
    "storybook": "start-storybook -p 7007",
    "test": "npx jest",
    "test:types": "tsc --noEmit",
    "test:unit": "npm run test --",
    "test:lint": "eslint -c ./.eslintrc.js ./src --ext .ts,.tsx",
    "test:ci": "cross-env CI=true run-s test:types test:lint test:unit"
  },
  "dependencies": {
    "@expo/vector-icons": "^12.0.5",
    "@react-native-async-storage/async-storage": "^1.16.1",
    "@react-navigation/bottom-tabs": "^6.2.0",
    "@react-navigation/drawer": "^6.3.1",
    "@react-navigation/native": "^6.0.8",
    "@react-navigation/native-stack": "^6.5.0",
    "@react-navigation/stack": "^6.1.1",
    "expo": "~44.0.2",
    "expo-splash-screen": "~0.14.1",
    "expo-status-bar": "~1.2.0",
    "native-base": "^3.3.7",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-native": "0.64.3",
    "react-native-dotenv": "^3.3.1",
    "react-native-gesture-handler": "~2.1.0",
    "react-native-reanimated": "~2.3.1",
    "react-native-safe-area-context": "3.3.2",
    "react-native-screens": "~3.10.1",
    "react-native-storybook-loader": "^2.0.5",
    "react-native-svg": "^12.1.1",
    "react-native-svg-transformer": "^1.0.0",
    "react-native-web": "0.17.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@cremalab/scripts": "^0.11.0",
    "@storybook/addon-actions": "^5.3.21",
    "@storybook/addon-knobs": "^5.3.21",
    "@storybook/addon-links": "^5.3.21",
    "@storybook/addon-ondevice-actions": "^5.3.23",
    "@storybook/addon-ondevice-knobs": "^5.3.25",
    "@storybook/react-native": "^5.3.25",
    "@storybook/react-native-server": "^5.3.23",
    "@testing-library/jest-native": "^4.0.4",
    "@testing-library/react-native": "^9.0.0",
    "@types/jest": "^27.4.1",
    "@types/react": "^17.0.39",
    "@types/react-native": "^0.66.16",
    "@types/react-native-vector-icons": "^6.4.10",
    "@types/react-router-native": "^5.1.3",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "babel-loader": "^8.2.3",
    "cross-env": "^7.0.3",
    "cross-spawn": "^7.0.3",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-config-react-app": "^7.0.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-react-native": "^4.0.0",
    "husky": "^7.0.4",
    "hygen": "^6.2.0",
    "icon-set-creator": "^0.1.4",
    "jest": "^26.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "react-native-typescript-transformer": "^1.2.13",
    "react-test-renderer": "^17.0.2",
    "trash-cli": "^5.0.0",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  },
  "private": true,
  "name": "app-mobile-2.0",
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./src",
        "./packages"
      ],
      "pattern": "**/*stories.tsx",
      "outputFile": "./storybook/storyLoader.js"
    }
  }
}
